{
  "name": "org",
  "private": true,
  "type": "module",
  "repository": "https://github.com/Effect-TS/starter.git",
  "author": "Michael Arnaldi <arnaldimichael@gmail.com>",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "node packages/app/build/esm/main.js",
    "clean": "ultra -r clean && rimraf tsconfig.tsbuildinfo",
    "build": "tsc -b tsconfig.json",
    "build-watch": "yarn build --watch",
    "circular": "yarn org:madge --ts-config ./tsconfig.madge.json --circular --no-color --no-spinner --warning packages/*/build/esm packages/*/build/test",
    "test": "vitest",
    "autofix": "ultra -r autofix",
    "lint": "ultra -r lint",
    "tc": "ultra -r tc",
    "postinstall": "tsplus-install",
    "org:rimraf": "cd $INIT_CWD && rimraf",
    "org:tsc": "cd $INIT_CWD && tsc",
    "org:eslint": "cd $INIT_CWD && eslint",
    "org:concurrently": "cd $INIT_CWD && concurrently",
    "org:build-utils": "cd $INIT_CWD && build-utils",
    "org:babel": "cd $INIT_CWD && babel",
    "org:madge": "cd $INIT_CWD && madge"
  },
  "packageManager": "yarn@3.2.1",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@effect-ts/build-utils": "0.40.3",
    "@effect-ts/core": "^0.60.2",
    "@repo-tooling/eslint-plugin-dprint": "^0.0.4",
    "@tsplus/installer": "^0.0.129",
    "@types/node": "^18.6.3",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "c8": "^7.12.0",
    "concurrently": "^7.3.0",
    "cpx": "^1.5.0",
    "eslint": "^8.21.0",
    "eslint-import-resolver-typescript": "^3.4.0",
    "eslint-plugin-codegen": "0.16.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "madge": "^5.0.1",
    "picocolors": "^1.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4",
    "ultra-runner": "^3.10.5",
    "vite": "^3.0.4",
    "vitest": "0.20.3"
  },
  "resolutions": {
    "eslint-plugin-codegen": "patch:eslint-plugin-codegen@npm:0.16.1#.yarn/patches/eslint-plugin-codegen-npm-0.16.1-87770191cd"
  }
}
